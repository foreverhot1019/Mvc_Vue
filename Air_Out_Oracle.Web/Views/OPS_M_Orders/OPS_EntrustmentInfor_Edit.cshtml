    $(function () {
        var intervalwt = setInterval(function () {
            try {
                var $ArrTxtNotify_Part_H = $("input[name='Notify_Part_H'].textbox-value", "#fdxx_@i");
                $.each($ArrTxtNotify_Part_H, function(){
