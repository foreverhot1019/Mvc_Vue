                    if($Id == "0"){
                        $("#btnwtxx_@i", "#wtxx_@i").attr("disabled", "disabled")
                    }
                    clearInterval(intervalwt);
                });
            } catch (e) {
                console.log(e);
                clearInterval(intervalwt);
            }
        }, 50);
    });
