                    }
                    FromDataWTXX = $.extend(FromDataWTXX,FormNAMEData);
                    initaadd("#wtxx_@i", @i, FromDataWTXX);//
                    //点击委托信息二中的绑定按钮
                    var tab = "btnwtxx_@i";
                    $('#' + tab).on("click", function (e) {
                        var opsid = $("#Id","#wtxx_@i").val();
                        is_bindingwarehouse = "1";
                        bindingwarehouse(true,e.pageX,e.pageY);//打开绑定仓库接单信息选择框
