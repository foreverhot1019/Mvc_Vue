@model AirOut.Web.Models.Finance
<table id="finance_datagrid" class="easyui-datagrid" title="" style="width:auto; height:auto; max-height:465px;min-height:300px;"
       data-options="
	   rownumbers:true,
	   checkOnSelect:false,
	   selectOnCheck:false,
	   idField:'Id',
	   sortName:'Id',
	   sortOrder:'desc',
	   remoteFilter: true,
	   singleSelect: true,
	   toolbar: '#finance_toolbar',
	   method: 'post',
       pageList: [10, 100, 200, 500, 1000, 2000, 5000],
	   //onClickCell: onClickCell,
       //onBeginEdit:onBeginEdit,
	   //onSelect:onSelect,
       //showFooter:true,
	   pagination: 'true'">
    <thead>
        <tr>
            <th data-options="field:'ck',checkbox:true"></th>
            <th data-options="field:'Dzbh',width:80,sortable:true,resizable:true,
            formatter:DzbhFormatter,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,20]'
            	}
            }">@Html.DisplayNameFor(model => model.Dzbh)</th>
            <th data-options="field:'Bill_Type',width:70,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,20]'
            	}
            }">@Html.DisplayNameFor(model => model.Bill_Type)</th>
            <th data-options="field:'Line_No',width:50,align:'right',sortable:true,resizable:true,
            editor:{
            	type:'numberbox',
            	options:{
            		required:true,
                    precision:0
            	}
            }">@Html.DisplayNameFor(model => model.Line_No)</th>
            <th data-options="field:'Money_Code',width:60,sortable:true,resizable:true,
            formatter:joinformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false,
                    validType:'length[0,20]'
            	}
            }">@Html.DisplayNameFor(model => model.Money_Code)</th>
            <th data-options="field:'Bill_Account2',width:70,align:'right',sortable:true,resizable:true,
            editor:{
            	type:'numberbox',
            	options:{
            		required:true,
                    precision:2
            	}
            }">@Html.DisplayNameFor(model => model.Bill_Account2)</th>
            <th data-options="field:'Bill_Amount',width:70,align:'right',sortable:true,resizable:true,
            editor:{
                type:'numberbox',
                options:{
                    required:true,
                    precision:2
                }
            }">@Html.DisplayNameFor(model => model.Bill_Amount)</th>
            <th data-options="field:'Bill_TaxAmount',width:70,align:'right',sortable:true,resizable:true,
            editor:{
                type:'numberbox',
                options:{
                    required:true,
                    precision:2
                }
            }">@Html.DisplayNameFor(model => model.Bill_TaxAmount)</th>
            <th data-options="field:'Bill_AmountTaxTotal',width:70,align:'right',sortable:true,resizable:true,
            editor:{
                type:'numberbox',
                options:{
                    required:true,
                    precision:2
                }
            }">@Html.DisplayNameFor(model => model.Bill_AmountTaxTotal)</th>
            <th data-options="field:'Bill_Object_Id',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,50]'
            	}
            }">结算方</th>
            <!--Bill_Object_Name-->
            <th data-options="field:'Bill_Object_IdNAME',width:150,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,20]'
            	}
            }">结算方名称</th>
            <!--开航日期 从委托中带-->
            <th data-options="field:'Flight_Date_Want',width:75,align:'right',sortable:true,resizable:true,
            formatter:dateformatter,
            editor:{
            	type:'datebox',
            	options:{
            		required:false
                }
            }">开航日期</th>
            <th data-options="field:'Bill_Date',width:125,align:'right',sortable:true,resizable:true,
            formatter:datetimeformatter,
            editor:{
            	type:'datetimebox',
            	options:{
            		required:false
                }
            }">@Html.DisplayNameFor(model => model.Bill_Date)</th>
            <th data-options="field:'BillEDITWHO',width:80,sortable:true,resizable:true,
            formatter:joinformatter,
            editor:{
                type:'textbox',
                options:{
                    required:false,
                    validType:'length[0,20]'
                }
            }">@Html.DisplayNameFor(model => model.BillEDITWHO)</th>
            <th data-options="field:'BillEDITTS',width:120,align:'right',sortable:true,resizable:true,
            formatter:datetimeformatter,
            editor:{
                type:'datetimebox',
                options:{
                    required:false
                }
            }">@Html.DisplayNameFor(model => model.BillEDITTS)</th>
            <th data-options="field:'Payway',width:80,sortable:true,resizable:true,
            formatter:joinformatter,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,20]'
            	}
            }">@Html.DisplayNameFor(model => model.Payway)</th>
            <th data-options="field:'Remark',width:120,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,200]'
            	}
            }">@Html.DisplayNameFor(model => model.Remark)</th>
            <th data-options="field:'MBL',width:100,sortable:true,resizable:true,
            formatter:MBLFormatter,
            editor:{
                type:'textbox',
                options:{
                    required:false,
                    validType:'length[0,20]'
                }
            }">@Html.DisplayNameFor(model => model.MBL)</th>
            <!--目的港 从委托中带-->
            <th data-options="field:'End_Port',width:80,sortable:true,resizable:true,
            formatter:joinformatter,
            editor:{
                type:'textbox',
                options:{
                    required:false,
                    validType:'length[0,20]'
                }
            }">目的港</th>
            <th data-options="field:'AuditStatus',width:80,sortable:true,resizable:true,
            formatter:UsedformatterByArrAuditStatus,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.AuditStatus)</th>
            <th data-options="field:'Create_Status',width:80,sortable:true,resizable:true,
            formatter:function(value, row, index){return formatterByEnumArray(value, row, index, ArrBms_BillCreate_Status);},
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.Create_Status)</th>
            <th data-options="field:'Cancel_Status',width:80,sortable:true,resizable:true,
            formatter:boolformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.Cancel_Status)</th>
            <th data-options="field:'ECC_Status',width:80,sortable:true,resizable:true,
            formatter:boolformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.ECC_Status)</th>
            <th data-options="field:'ECC_InvoiceSendDate',width:100,sortable:true,resizable:true,
            formatter:datetimeformatter,
            editor:{
            	type:'datetimebox',
            	options:{
            		required:false
            	}
            }">发送ECC时间</th>
            <th data-options="field:'ECC_InvoiceRecvDate',width:100,sortable:true,resizable:true,
            formatter:datetimeformatter,
            editor:{
            	type:'datetimebox',
            	options:{
            		required:false
            	}
            }">接收ECC时间</th>
            <th data-options="field:'ECC_StatusMsg',width:150,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,500]'
            	}
            }">@Html.DisplayNameFor(model => model.ECC_StatusMsg)</th>
            <th data-options="field:'Sumbmit_Status',width:80,sortable:true,resizable:true,
            formatter:boolformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.Sumbmit_Status)</th>
            <th data-options="field:'Sumbmit_No',width:105,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Sumbmit_No)</th>
            <th data-options="field:'Sumbmit_Name',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Sumbmit_Name)</th>
            <th data-options="field:'Sumbmit_Date',width:80,sortable:true,resizable:true,
            formatter:dateformatter,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Sumbmit_Date)</th>
            <th data-options="field:'Sumbmit_ECCNo',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.Sumbmit_ECCNo)</th>
            <th data-options="field:'SignIn_Status',width:80,sortable:true,resizable:true,
            formatter:boolformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.SignIn_Status)</th>
            <th data-options="field:'SignIn_No',width:110,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.SignIn_No)</th>
            <th data-options="field:'SignIn_ECCNo',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.SignIn_ECCNo)</th>
            <th data-options="field:'Invoice_Status',width:80,sortable:true,resizable:true,
            formatter:boolformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_Status)</th>
            <th data-options="field:'Invoice_No',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_No)</th>
            <th data-options="field:'Invoice_MoneyCode',width:80,sortable:true,resizable:true,
            editor:{
            	type:'combogrid',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_MoneyCode)</th>
            <th data-options="field:'Invoice_FeeType',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_FeeType)</th>
            <th data-options="field:'Invoice_Name',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_Name)</th>
            <th data-options="field:'Invoice_Date',width:80,sortable:true,resizable:true,
            formatter:dateformatter,
            editor:{
            	type:'textbox',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_Date)</th>
            <th data-options="field:'Invoice_Remark',width:238,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
            		required:false,
                    validType:'length[0,200]'
            	}
            }">@Html.DisplayNameFor(model => model.Invoice_Remark)</th>
            <th data-options="field:'SellAccount_Status',width:80,sortable:true,resizable:true,
            formatter:boolformatter,
            editor:{
            	type:'combogrid',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.SellAccount_Status)</th>
            <th data-options="field:'SellAccount_Name',width:80,sortable:true,resizable:true,
            editor:{
            	type:'textbox',
            	options:{
                    required:false,
                    validType:'length[0,50]'
            	}
            }">@Html.DisplayNameFor(model => model.SellAccount_Name)</th>
            <th data-options="field:'SellAccount_Date',width:80,sortable:true,resizable:true,
            formatter:dateformatter,
            editor:{
            	type:'textbox',
            	options:{
            		required:false
            	}
            }">@Html.DisplayNameFor(model => model.SellAccount_Date)</th>
        </tr>
    </thead>
</table>