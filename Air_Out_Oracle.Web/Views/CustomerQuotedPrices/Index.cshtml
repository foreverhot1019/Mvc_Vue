                FromCache = "_FromCache";
            //状态
            var $Status = $('#Status');
            if($Status.length>0){
                $Status.combogrid($.extend({},combogrid_DefaultSettings,{
                    mode: 'loacl',
                    pagination: false,
                    data:ArrStatus
                }));
            }
            //审批状态
            var $AuditStatus = $('#AuditStatus');
            if($AuditStatus.length>0){
                $AuditStatus.combogrid($.extend({},combogrid_DefaultSettings,{
                    mode: 'loacl',
                    pagination: false,
