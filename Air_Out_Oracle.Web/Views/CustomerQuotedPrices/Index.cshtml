        //获取选中行ID
        function getSelectDatagrid(rows) {
            var ids = [];
            for (var i = 0; i < rows.length; i++) {
                if (rows[i].AuditStatus == "0" || rows[i].AuditStatus == "-1") {
                    ids.push(rows[i].Id);
                }
            }
            return ids;
