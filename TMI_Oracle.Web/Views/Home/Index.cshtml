            <span class="label label-success pull-right">当月</span>
            <h5>制单</h5>
        </div>
        <div class="ibox-content">
            <ul class="stat-list">
                <li>
                    <h2 class="no-margins">12</h2>
                    <small>当月制单数量</small>
                    <div class="stat-percent">10 <i class="fa fa-level-down text-warning"></i></div>

                    <div class="progress progress-striped active progress-mini">
                        <div class="stat-percent">10 <i class="fa fa-bolt text-danger"></i></div>
                        <div class="progress progress-striped active progress-mini">
                            <div class="progress-bar progress-bar-success"
                                 style="width: 10%;" title="上月总数 10">
                            </div>
                            <div class="progress-bar progress-bar-info"
                                 style="width: 10%;" title="当月总数 10">
                            </div>
                            <div class="progress-bar progress-bar-navy"
                                 style="width: 90%;" title="当月增长 10%">
                            </div>
                        </div>
                    </div>
                </li>
                <li title="进口业务-当月：10 上月：10">
                    <h2 class="no-margins ">12</h2>
                    <small>进口业务</small>
                    <div class="stat-percent">10 <i class="fa fa-bolt text-danger"></i></div>
                    <div class="progress progress-striped active progress-mini">
                        <div class="progress-bar progress-bar-success"
